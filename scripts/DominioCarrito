USE [PICA];

GO

create SCHEMA carrito;

GO

--drop table carrito.carrito;

--drop table carrito

create table carrito.carrito
(
    id int,
    fecha nvarchar(100),
    usuario nvarchar(100)
)

GO

create table carrito_producto
(
    carrito_id int,
    producto_id int,
    producto_codigo nvarchar(100),
    producto_fabricante nvarchar(100),
    cantidad int,
    precio float
)



